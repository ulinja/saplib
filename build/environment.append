
#@SAPLING_START
# default programs
EDITOR=nvim
VISUAL=nvim
PAGER=bat --paging=always

# misc environment variables
HISTFILESIZE=10000

# you can source this variable to access saplib's bash functions
SAPLIB_BASH=/usr/local/lib/saplib/bash/saplib.bash

# controls globally whether saplib's debug messages are printed
SAPLIB_DEBUGMESSAGES=false
#@SAPLING_END
