
#@SAPLING_START
# source saplib's bash aliases
source /usr/local/lib/saplib/bash/src/aliases.bash

# source saplib default prompt
source /usr/local/lib/saplib/bash/src/prompt.bash

# cargo config directory
export CARGO_HOME=$HOME/.config/cargo
# cargo binary installation target directory
export CARGO_INSTALL_ROOT=$HOME/.local
#@SAPLING_END
